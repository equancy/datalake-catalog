        "list": []
    },
    "storage": {
        "path": {
            "pattern": "{domain}/{provider}/{feed}/{date}/{date}_{feed}.csv",
            "params": {
                "date": "YYYY-MM-DD"
            }
        },
        "stores": [
            "stronghold",
            "pseudonymized"
        ]
    },
    "columns": [
        {
            "name": "column1",
