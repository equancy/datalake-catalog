        },
        {
            "name": "column2",
            "type": "number",
            "gdpr": {
                "pii": true,
                "drop": false
            },
            "dqm": {
                "drop_on_error": "value",
                "controls": [
                    {
                        "function": ["argv"]
                    }
                ]
            }
        }
    ],
    "gdpr": {
        "anonymization": {
            "from": "column_id" 
